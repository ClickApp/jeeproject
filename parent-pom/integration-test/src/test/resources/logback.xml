<?xml version="1.0" encoding="UTF-8"?>

<configuration>

<!-- SLF4J levels: ERROR, WARN, INFO, DEBUG, TRACE -->

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{5} - %msg [Line %L]%n</pattern>  
    </encoder>
  </appender>
    
  <root level="DEBUG">          
    <appender-ref ref="STDOUT" />
  </root>  
  
</configuration>
